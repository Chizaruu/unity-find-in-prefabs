{
    "name": "unity-find-in-prefabs",
    "displayName": "Unity Find In Prefabs",
    "description": "Find prefabs which depend on the C# file",
    "version": "1.0.6",
    "publisher": "jarvisjiang",
    "author": {
        "name": "Jarvis Jiang",
        "email": "jiang115jie@gmail.com"
    },
    "repository": "https://github.com/JiangJie/unity-find-in-prefabs",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "unify",
        "prefab",
        "csharp",
        "C#"
    ],
    "activationEvents": [
        "onCommand:unity-find-in-prefabs"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "unity-find-in-prefabs",
                "title": "Unity: Find C# In Prefabs"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "unity-find-in-prefabs",
                    "when": "editorLangId == csharp"
                }
            ],
            "editor/context": [
                {
                    "when": "editorFocus",
                    "command": "unity-find-in-prefabs"
                }
            ],
            "editor/title/context": [
                {
                    "command": "unity-find-in-prefabs"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "npm run compile-tests && npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.63.0",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "@typescript-eslint/eslint-plugin": "^5.9.1",
        "@typescript-eslint/parser": "^5.9.1",
        "eslint": "^8.6.0",
        "glob": "^7.2.0",
        "mocha": "^9.1.3",
        "typescript": "^4.5.4",
        "ts-loader": "^9.2.6",
        "webpack": "^5.66.0",
        "webpack-cli": "^4.9.1",
        "@vscode/test-electron": "^2.0.3"
    }
}